{
  "name": "muckr-web",
  "version": "0.1.0",
  "description": "Web app for muckr",
  "main": "src/server/index.js",
  "repository": "https://github.com/cjolowicz/muckr-web.git",
  "homepage": "https://github.com/cjolowicz/muckr-web#muckr-web",
  "bugs": "https://github.com/cjolowicz/muckr-web/issues",
  "author": "Claudio Jolowicz <mail@claudiojolowicz.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src __tests__ webpack.config.babel.js",
    "test": "jest --coverage",
    "fulltest": "yarn run lint && yarn run flow && yarn run test",
    "submit-coverage": "coveralls < coverage/lcov.info",
    "build": "yarn run compile && yarn run bundle",
    "compile": "babel --delete-dir-on-start src --out-dir lib",
    "bundle": "webpack -p --progress --env.NODE_ENV=production",
    "bundle-server": "webpack-dev-server --progress",
    "heroku-postbuild": "yarn run build",
    "start": "nodemon --ignore lib --ignore dist --exec babel-node ."
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "coveralls": "^3.0.2",
    "eslint": "^5.11.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "flow-bin": "^0.89.0",
    "flow-typed": "^2.5.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "supertest": "^3.3.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "compression": "^1.7.3",
    "express": "^4.16.4"
  }
}
