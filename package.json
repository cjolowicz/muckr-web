{
  "name": "muckr-web",
  "version": "0.12.0",
  "description": "Web app for muckr",
  "main": "src/server/index.js",
  "repository": "https://github.com/cjolowicz/muckr-web.git",
  "homepage": "https://github.com/cjolowicz/muckr-web#muckr-web",
  "bugs": "https://github.com/cjolowicz/muckr-web/issues",
  "author": "Claudio Jolowicz <mail@claudiojolowicz.com>",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write '{.,src/**}/*.{js,json}'",
    "test": "run-s -y test:*",
    "test:lint": "eslint src webpack.config.babel.js",
    "test:flow": "flow",
    "test:unit": "env `cat .env.example` jest --coverage src",
    "coverage:submit": "coveralls < coverage/lcov.info",
    "build": "run-s -y build:*",
    "build:babel": "babel --delete-dir-on-start src --out-dir lib --ignore '**/__tests__'",
    "build:webpack": "webpack -p --env.production",
    "heroku-postbuild": "yarn run build",
    "start": "node lib/server",
    "start:dev": "run-p -y start:dev:*",
    "start:dev:nodemon": "nodemon --ignore lib --ignore dist --exec babel-node .",
    "start:dev:webpack": "webpack-dev-server --env.production=false"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/node_modules/regenerator-runtime/runtime"
    ],
    "setupFilesAfterEnv": [
      "react-testing-library/cleanup-after-each",
      "jest-enzyme"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.2",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "flow-bin": "^0.99.1",
    "flow-typed": "^2.5.2",
    "history": "^4.9.0",
    "jest": "^24.8.0",
    "jest-environment-enzyme": "^7.0.1",
    "jest-enzyme": "^7.0.1",
    "nodemon": "^1.19.1",
    "prettier": "1.17.1",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^7.0.0",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.1.14",
    "yarn-run-all": "^3.1.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "compression": "^1.7.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jss": "^9.8.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-jss": "^8.6.1",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "^0.0.54",
    "universal-cookie": "^4.0.0",
    "universal-cookie-express": "^4.0.0"
  }
}
