{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "flowtype",
    "compat"
  ],
  "rules": {
    "compat/compat": 2
  },
  "settings": {
    "polyfills": [
      "fetch"
    ]
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
