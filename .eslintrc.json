{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "flowtype",
    "compat",
    "react"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "no-param-reassign": ["error", { "props": false }],
    "react/jsx-one-expression-per-line": ["off"],
    "compat/compat": "error"
  },
  "overrides": {
    "files": ["**/__tests__/**"],
    "rules": {
      "compat/compat": "off"
    }
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
