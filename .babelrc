{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "entry",
        targets: {
          "node": "current"
        }
      }
    ],
    "@babel/flow",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
